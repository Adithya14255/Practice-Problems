import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a,b;
        a = sc.nextInt();
        b = sc.nextInt();
        int temp = 0;
        int ans = 0;
        for(int i=0;i<a;i++){
            temp = sc.nextInt();
            ans+=1;
            if(temp>b){
                ans+=1;
            }
        }
        System.out.println(ans);
    }
}
