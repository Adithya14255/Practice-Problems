class Solution {
    public boolean isValid(String s) {
        int[] arr = new int[s.length()+1];
        int top = 0;
        for(int i=0;i<s.length();i++){
            if (top<0) break;
            if(s.charAt(i) == '(') arr[++top] = 1;
            else if(s.charAt(i) == '[') arr[++top] = 2;
            else if(s.charAt(i) == '{') arr[++top] = 3;
            else if(s.charAt(i) == ')'){
                if(top<0 || arr[top--] != 1)
             return false;
            }
            else if(s.charAt(i) == ']'){
                if(top<0 || arr[top--] != 2)
             return false;
            }
            else if(s.charAt(i) == '}'){
                if(top<0 || arr[top--] != 3)
             return false;
            }

        }
        if(top == 0) return true;
        return false;
    }
}
