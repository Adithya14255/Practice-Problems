class Solution {
    public int lengthOfLastWord(String s) {
        int c = 0;
        int flag = 0;
        
        for(int i = s.length()-1;i>=0;i--){
            if(flag == 0 && s.charAt(i) == ' '){
                continue;
            }else{
                flag = 1;
            }
            if (flag==1 && s.charAt(i) == ' ') return c;
            c++;
        }
        return c;
    }
}
