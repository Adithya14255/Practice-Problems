class Solution {
    public int firstUniqChar(String s) {
        List<Character> hs = new ArrayList<>();
        int[] alphabetCount = new int[26];
        char[] arr = s.toCharArray();
        for(char x:arr){
            alphabetCount[x-'a']++;
        }
        for(int i=0;i<s.length();i++){
            if(alphabetCount[arr[i]-'a']==1){
                return i;
            }
        }

        return -1;
    }
}
