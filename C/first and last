#include <stdio.h>

int main()
{
    int arr[] = {1,2,3,3,5,6,1,2};
    int first=-1,last = -1;
    int key = 1;
    for(int i=0;i<8;i++){
        if(arr[i] == key){
            if(first==-1){
                first = i;
            }
            last = i;
        }
    }
    printf("first = %d \nlast = %d",first,last);

    return 0;
}
