#include <stdio.h>


int main()
{
    int arr[10] = {4,5,2,2,1,7,3,0};
    int seen[100] = {0};
    int seentwice[100] = {0};
    for (int i=0;i<8;i++){
        if (seen[arr[i]]) seentwice[arr[i]] = 1;
        seen[arr[i]] = 1;
        
    }
    for (int i=0;i<8;i++){
        if(!seentwice[arr[i]]) printf("%d ",arr[i]);
    }
    
   
    
    return 0;
}
