#include <stdio.h>

void reverse(int* arr,int l, int r){
	int temp;
	while(l<r){
			temp=arr[l];
			arr[l]=arr[r];
			arr[r] = temp;
			l++;
			r--;
		}
}

int main(){
	int num,n,k,temp;
	scanf("%d", &num);
	for(int m=0;m<num;m++){      
		scanf("%d %d",&n,&k);
		int a[n];
		for(int l=0;l<n;l++) scanf("%d",&a[l]);
		k=k%n;
		reverse(a,0,n-1);
		reverse(a,0,k-1);
		reverse(a,k,n-1);
		for (int i=0;i<n;i++)
		printf("%d ",a[i]);
		printf("\n");
	}
	return 0;
}
