
#include <stdio.h>

int main()
{
    int n,j=0,flag=0;
    scanf("%d",&n);
    char str[n][101];
    for(int i=0;i<n;i++){
        scanf("%s",str[i]);
    }
    for(int i=0;i<n;i++){
        flag = 0;
        for(int j=0;j<strlen(str[i])-1;j++){
            if(str[i][j] == str[i][j+1]){
                flag=1;
                break;
            }
        }
        printf("%d\n",flag?1:strlen(str[i]));
    }
    
    return 0;
}
