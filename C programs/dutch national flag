#include <stdio.h>

int main()
{
    int arr[13] = {1,0,1,1,1,2,2,0,0,2,2,1,1};
    int low=0,mid=0,high=12,temp;
    while(mid<=high){
        if(arr[mid] == 1){
            mid++;
        }
        else if(arr[mid]<1){
            temp = arr[mid];
            arr[mid] = arr[low];
            arr[low] = temp;
            mid++;
            low++;
        }
        else{
            temp = arr[mid];
            arr[mid] = arr[high];
            arr[high] = temp;
            high--;
        }
        
    }
    for(int i=0;i<13;i++){
        printf("%d ",arr[i]);
    }
    return 0;
