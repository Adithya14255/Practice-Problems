#include <ctype.h>
#include <string.h>
#include <stdio.h>

int main()
{
    int j=0;
    char ca='a',cb='a';
    char a[101],b[101];
    scanf("%s",a);
    scanf("%s",b);
    while(j<(int)strlen(a)){
        if(tolower(a[j]) != tolower(b[j])){
            ca = tolower(a[j]);
            cb = tolower(b[j]);
            break;
        }
        j++;
    }
    if(ca==cb){
        printf("0");
    }
    else if(ca>cb){
        printf("1");
    }else{
        printf("-1");
    }
        
    
    return 0;
}
