#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n;
        scanf("%d", &n);
        int arr[n][n];

        // read matrix
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                scanf("%d", &arr[i][j]);
            }
        }

        int count = 0;

        // check all pairs
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                for (int p = i; p < n; p++) {
                    for (int q = j; q < n; q++) {
                        if ((i != p || j != q) && arr[i][j] > arr[p][q]) {
                            count++;
                        }
                    }
                }
            }
        }

        printf("%d\n", count);
    }
    return 0;
}
