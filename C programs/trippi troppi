
#include <stdio.h>

int main()
{
    int n,j=0;
    
    char abb[3];
    scanf("%d",&n);
    char str[n][100];
    for(int i=0;i<n;i++){
        scanf("\n%[^\n]*c",str[i]);
    }
    for(int i=0;i<n;i++){
        abb[0] = str[i][0];
        j=1;
        for(int k=1;k<3;k++){
            while(str[i][j]!=' ') j++;
            abb[k] = str[i][++j];
            j++;
        }
        printf("%s\n",abb);
    }
    
    return 0;
}
