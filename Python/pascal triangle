class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        nums = []
        for i in range(numRows):
            new_row = [1]*(i+1)
            for j in range(1,i):
                new_row[j] = nums[-1][j-1] + nums[-1][j] 
            nums.append(new_row)
        return nums
