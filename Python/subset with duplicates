class Solution:
    def subsetsWithDup(self, nums: List[int]) -> List[List[int]]:
        nums.sort()
        power_set = []
        def get_subset(start,path):
            power_set.append(path[:])
            for i in range(start,len(nums)):
                if i>start and nums[i] == nums[i-1]:
                    continue
                path.append(nums[i])
                get_subset(i+1,path)
                path.pop()
        get_subset(0,[])
        return power_set           
