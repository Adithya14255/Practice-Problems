class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        # a = sorted(nums)
        # i=0
        # while i<len(nums)-1:
        #     if a[i] != a[i+1]:
        #         return a[i]
        #     i+=2
        # return a[-1]
        a = 0
        for i in nums:
            a ^= i
        return a
