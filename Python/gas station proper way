class Solution:
    def canCompleteCircuit(self, gas: List[int], cost: List[int]) -> int:
        diff = []
        for i in range(len(gas)):
            diff.append(gas[i] - cost[i])
        if sum(diff) < 0:
            return -1
        start = 0
        tank = 0
        for i in range(len(gas)):
            tank += diff[i]
            if tank<0:
                tank = 0
                start = i+1
                
        return start 

