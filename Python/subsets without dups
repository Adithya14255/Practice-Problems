class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
        res = []
        def get_subset(start,path):
            res.append(path[:])
            for i in range(start,len(nums)):
                path.append(nums[i])
                get_subset(i+1,path)
                path.pop()
        get_subset(0,[])
        return res
