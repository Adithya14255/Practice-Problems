class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        # i = 0
        # while i < len(nums):
        #     if nums[i] == target:
        #         return i
        #     if nums[i] > target:
        #         return i
        #     i += 1
        # return i
        left,right = 0,len(nums)-1
        while left<=right:
            mid = (left + right)//2
            if nums[mid] == target:
                return mid
            elif nums[mid] > target:
                right = mid - 1
            else:
                left = mid + 1
        return left 
