class Solution:
    def reverseVowels(self, s: str) -> str:
        l,r = 0,len(s)-1
        s = [i for i in s]
        vowels = set("aeiouAEIOU")
        while l<r:
            if s[l] not in vowels:
                l+=1
                continue
            if s[r] not in vowels:
                r-=1
                continue
            s[l],s[r] = s[r],s[l] 
            r-=1
            l+=1
        return "".join(s)
