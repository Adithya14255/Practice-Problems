class Solution:
    def maxSlidingWindow(self, nums: List[int], k: int) -> List[int]:
        max_window = []
        dq = deque()
        for i,n in enumerate(nums):
            while dq and dq[0] < i-k+1:
                dq.popleft()
            
            while dq and nums[dq[-1]] < n:
                dq.pop()

            dq.append(i)
            if i>=k-1:
                max_window.append(nums[dq[0]])
        
        return max_window
